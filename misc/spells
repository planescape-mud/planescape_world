ЗАКЛИНАНИЕ 13
  НАЗВАНИЕ "святая кара"
  СИНОНИМ "orthae zotreth"
  ИНДЕФИКАТОР "СВЯТАЯ_КАРА"
  ПРОЦЕДУРА "spell_holy_smite"
  ШКОЛА ДОБРА : МАНА 15 : УРОВЕНЬ 1 : УМЕНИЕ 50 : СВРЕД 1+3+1+4 : ТВРЕД ПОЗИТИВ
  АГРЕССИЯ ВСЕ : ЦЕЛЬ ПЕРСОНА_ЛОКАЦИЯ+ПРОТИВНИК_ЖЕРТВЫ : ПОЛОЖЕНИЕ СРАЖАЕТСЯ
  ЗАДЕРЖКА 2 : ЗАПОМИНАНИЕ 3 : СОХРАНЕНИЕ 20
  ПРОИЗНОШЕНИЕ (
      ИГРОК "Вытянув руку по направлению к 2+д, Вы прошептали:^'%s'."
      ЖЕРТВА "Вытянув руку в Вашу сторону, 1+и прошептал1(,а,о,и):^'%s'."
      ЛОКАЦИЯ "Вытянув руку в сторону 2+р, 1+и прошептал1(,а,о,и):^'%s'."
      )
  АСООБЩЕНИЯ (
      УДАР_ИГРОК "Божественное сияние ослепило 2+в."
      УДАР_ЖЕРТВА "Божественное сияние ослепило Вас."
      УДАР_ЛОКАЦИЯ "Божественное сияние ослепило 2+в."
      )
  БСООБЩЕНИЯ (
      УБИТ_ИГРОК "Вызванное Вами божественное сияние испепелило 2+в."
      УБИТ_ЖЕРТВА "Божественное сияние, вызванное 1+т, испепелило Вас."
      УБИТ_ЛОКАЦИЯ "Божественное сияние, вызванное 1+т, испепелило 2+в."
      УДАР_ИГРОК "Вызванное Вами божественное сияние #0обожгло 2+в."
      УДАР_ЖЕРТВА "Вызванное 1+т божественное сияние #0обожгло Вас."
      УДАР_ЛОКАЦИЯ "Вызванное 1+т божественное сияние #0обожгло 2+в."
      ПРОМАХ_ИГРОК "Вызванное Вами божественное сияние не причинило 2+д вреда."
      ПРОМАХ_ЖЕРТВА "Вызванное 1+т божественное сияние не причинило Вам вреда."
      ПРОМАХ_ЛОКАЦИЯ "Вызванное 1+т божественное сияние не причинило 2+д вреда."
      БЕСМ_ИГРОК "Осторожно, 2+и сам2(,а,о,и) покарает Вас за такие фокусы."
      БЕСМ_ЖЕРТВА "1+и просил1(,а,о,и) своего бога покарать Вас, но ничего не добил1(ся,ась,ось,ись)."
      БЕСМ_ЛОКАЦИЯ "1+и просил1(,а,о,и) своего бога покарать 2+в, но ничего не добил1(ся,ась,ось,ись)."
      )

