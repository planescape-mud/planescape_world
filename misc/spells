;-----------------------------------------------------------------------
ЗАКЛИНАНИЕ 1
  НАЗВАНИЕ "опознание"
  СИНОНИМ "zhaun bol"
  ИНДЕФИКАТОР "ОПОЗНАТЬ"
  ПРОЦЕДУРА "spell_identify"
  ШКОЛА ПРОРИЦАНИЯ : МАНА 30 : УРОВЕНЬ 0 : УМЕНИЕ 0 : СВРЕД 0d0+0 : ТВРЕД НЕТ
  АГРЕССИЯ НЕТ
  ЦЕЛЬ ПЕРСОНА_ЛОКАЦИЯ+НАСЕБЯ+ПРЕДМЕТ_ИНВЕНТАРЬ+ПРЕДМЕТ_ЭКИПИРОВКА
  ПОЛОЖЕНИЕ СТОИТ
  ПРОИЗНОШЕНИЕ (
      ИГРОК "Сделав круговое движение руками, Вы прошептали:^'%s'."
      ЛОКАЦИЯ "Сделав круговое движение руками, 1+и прошептал1(,а,о,и):^'%s'."
      )
  АСООБЩЕНИЯ (
      УДАР_ЯИГРОК "Вы узнали свои характеристики."
      УДАР_ЯЛОКАЦИЯ "1+и узнал1(,а,о,и) свои характеристики."
      ПРОМАХ_ИГРОК "С помощью магии нельзя опознать другое существо."
      ПРОМАХ_ЖЕРТВА "1+и хотел1(,а,о,и) узнать Ваши характеристики."
      ПРОМАХ_ЛОКАЦИЯ "1+и хотел1(,а,о,и) узнать характеристики 2+р."
      )
  ОСООБЩЕНИЯ (
      УДАР_ИГРОК "Вы узнали свойства @1+р."
      УДАР_ЛОКАЦИЯ "1+и узнал1(,а,о,и) свойства @1+р."
      ПРОМАХ_ИГРОК "Информация о свойствах @1+р недоступна."
      )

